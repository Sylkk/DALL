<div class="ibox float-e-margins">
    <div class="ibox-title">
        <div class="btn-group pull-right">
                    <button id="btn_role_rep"  class="btn btn-primary btn-xs" type="button">Rôles</button>
                    <button id="btn_armor_rep" class="btn btn-white btn-xs" type="button">Armor</button>
                    <button id="btn_token_rep" class="btn btn-white btn-xs" type="button">Token</button>
                </div>
        <h5>Roster Info</h5>
    </div>
        
    <div id="rep_role" class="ibox-content">
        <div class="row">
            <div class="col-lg-12">
                <h3 id="title_role_rep" class="">Rôles representation</h3>
                <small>Pour un total de <b><%=chars.length%></b> membres : </small>
            </div>
            <div class="col-lg-6">
                <ul class="list-group clear-list m-t">
                    <li class="list-group-item fist-item">
                        <span class="pull-right text-danger">
                            ilvl.e <b><%= (tank_ilvl / tank.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-danger"><%if(tank.length < 10){%>0<%}%><%=tank.length%></span> Tank
                    </li>
                    <li class="list-group-item">
                        <span class="pull-right text-warning">
                            ilvl.e <b><%= (melee_ilvl / melee.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-warning"><%if(melee.length < 10){%>0<%}%><%=melee.length%></span> Melee
                    </li>
                    <li class="list-group-item">
                        <span class="pull-right text-success">
                            ilvl.e <b><%= (caster_ilvl / caster.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-success"><%if(caster.length < 10){%>0<%}%><%=caster.length%></span> Caster
                    </li>
                    <li class="list-group-item">
                        <span class="pull-right text-navy">
                            ilvl.e <b><%= (heal_ilvl / heal.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-primary"><%if(heal.length < 10){%>0<%}%><%=heal.length%></span> Healer
                    </li>
                </ul>
            </div>
            <div id="graph_role" class="col-lg-6">
                <div class="text-center">
                    <canvas id="roleDoughnutChart" width="78" height="78" style="width: 78px; height: 78px;"></canvas>
                    <h5>Rôles</h5>
                    <br/>
                    Average Item level: 
                    <br><b><h3><%= (tot_ilvl / chars.length ).toFixed(1) %></h3></b>
                </div>
            </div>
        </div>
    </div>
    
    <div id="rep_armor" class="ibox-content hidden">
        <div class="row">
            <div class="col-lg-12">
                <h3 id="title_armor_rep" class="">Armor representation</h3>
                <small>Pour un total de <b><%=chars.length%></b> membres : </small>
            </div>
            <div class="col-lg-6">
                <ul class="list-group clear-list m-t">
                    <li class="list-group-item fist-item">
                        <span class="pull-right text-danger">
                            ilvl.e <b><%= (tissu_ilvl / tissu.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-danger"><%if(tissu.length < 10){%>0<%}%><%=tissu.length%></span> Tissu
                    </li>
                    <li class="list-group-item">
                        <span class="pull-right text-warning">
                            ilvl.e <b><%= (cuir_ilvl / cuir.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-warning"><%if(cuir.length < 10){%>0<%}%><%=cuir.length%></span> Cuir
                    </li>
                    <li class="list-group-item">
                        <span class="pull-right text-success">
                            ilvl.e <b><%= (mail_ilvl / mail.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-success"><%if(mail.length < 10){%>0<%}%><%=mail.length%></span> Maille
                    </li>
                    <li class="list-group-item">
                        <span class="pull-right text-navy">
                            ilvl.e <b><%= (plaque_ilvl / plaque.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-primary"><%if(plaque.length < 10){%>0<%}%><%=plaque.length%></span> Plaque
                    </li>
                </ul>
            </div>
            <div id="graph_armor" class="col-lg-6">
                <div class="text-center">
                    <canvas id="armorDoughnutChart" width="78" height="78" style="width: 78px; height: 78px;"></canvas>
                    <h5>Armors</h5>
                    <br/>
                    Average Item level: 
                    <br><b><h3><%= (tot_ilvl / chars.length ).toFixed(1) %></h3></b>
                </div>
            </div>
        </div>
    </div>
    
    <div id="rep_token" class="ibox-content hidden">
        <div class="row">
            <div class="col-lg-12">
                <h3 id="title_token_rep" class="">Token representation</h3>
                <small>Pour un total de <b><%=chars.length%></b> membres : </small>
            </div>
            <div class="col-lg-6">
                <ul class="list-group clear-list m-t">
                    <li class="list-group-item fist-item">
                        <span class="pull-right text-danger">
                            ilvl.e <b><%= (vanquisher_ilvl / vanquisher.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-danger"><%if(vanquisher.length < 10){%>0<%}%><%=vanquisher.length%></span> Vanquisher
                    </li>
                    <li class="list-group-item">
                        <span class="pull-right text-warning">
                            ilvl.e <b><%= (protector_ilvl / protector.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-warning"><%if(protector.length < 10){%>0<%}%><%=protector.length%></span> Protector
                    </li>
                    <li class="list-group-item">
                        <span class="pull-right text-success">
                            ilvl.e <b><%= (conqueror_ilvl / conqueror.length).toFixed(1) %></b>
                        </span>
                        <span class="label label-success"><%if(conqueror.length < 10){%>0<%}%><%=conqueror.length%></span> Conqueror
                    </li>
                    <li class="list-group-item">
                        <span> &nbsp</span>
                    </li>
                    
                </ul>
            </div>
            <div id="graph_token" class="col-lg-6">
                <div class="text-center">
                    <canvas id="tokenDoughnutChart" width="78" height="78" style="width: 78px; height: 78px;"></canvas>
                    <h5>Tokens</h5>
                    <br/>
                    Average Item level: 
                    <br><b><h3><%= (tot_ilvl / chars.length ).toFixed(1) %></h3></b>
                </div>
            </div>
        </div>
    </div>
    
</div>

