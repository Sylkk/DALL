<script>
    $(document).ready(function() {
        getNewApplyNumber();  
    });
    
    
    function getNewApplyNumber(){
        return $.ajax({ 
            url: "/applys/newApplyNumber",
            type: 'GET',
            dataType: 'JSON'
        }).done(function(data) {
            if(data > 0)
                $("#newApplyNumber").html(data + " NEW");
        }).fail(function() {
            console.log("Something went wrong!");//gestion errors
        });
    }
    
